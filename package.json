{
	"name": "angular-todomvc",
	"version": "0.0.0",
	"license": "MIT",
	"scripts": {
		"ng": "ng",
		"dev": "ng serve",
		"build": "ng build --prod --optimization=false",
		"build:aot": "ngc && cp -rf dist/out-tsc/src/* aot/ && cd aot && ln -sf ../node_modules node_modules",
		"compile": "ngc",
		"start": "http-server dist",
		"start:aot": "es-dev-server --root-dir aot --app-index index.html --node-resolve",
		"test": "ng test",
		"lint": "ng lint",
		"format": "prettier --write .",
		"e2e": "ng e2e"
	},
	"private": true,
	"dependencies": {
		"@angular/animations": "7.2.6",
		"@angular/common": "7.2.6",
		"@angular/compiler": "7.2.6",
		"@angular/core": "7.2.6",
		"@angular/forms": "7.2.6",
		"@angular/http": "7.2.6",
		"@angular/platform-browser": "7.2.6",
		"@angular/platform-browser-dynamic": "7.2.6",
		"@angular/router": "7.2.6",
		"angular-autofocus-fix": "^0.1.0",
		"core-js": "^2.4.1",
		"reflect-metadata": "^0.1.13",
		"rxjs": "^6.4.0",
		"todomvc-app-css": "^2.0.0",
		"todomvc-common": "^1.0.1",
		"tslib": "^1.9.0",
		"zone.js": "^0.8.29"
	},
	"devDependencies": {
		"@angular-devkit/build-angular": "~15.0.0",
